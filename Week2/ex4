#include <stdio.h>



void funct(int* a, int* b){
	int k = *a;
	*a = *b;
	*b = k;
	
}

int main() {
	int a = 0;
	int b = 0;
	printf("a = "); 
	scanf("%d", &a);
	printf("b = "); 

	scanf("%d", &b);
	
	funct(&a, &b);
	
	printf("a = "); 
	printf("%d\n", a);
	
	printf("b = "); 
	printf("%d\n", b);
	
	return 0;
}
